package com.in28minutes.learnspringframework.game;

public class MarioGame implements GamingConsole {

	@Override
	public void up() {
		System.out.println("Jump");
	}
	@Override
	public void down() {
		System.out.println("go inside hole");
	}
	@Override
	public void right() {
		System.out.println("run");
	}
	@Override
	public void left() {
		System.out.println("stop");
	}
}
